{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "build": { 
                "command": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.js"
        }
    ]
}