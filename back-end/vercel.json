{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "build": "npm install && npm run build && npm run migration && npm run start"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.js"
        }
    ]
}